{
    "name": "yii-extension/user-view-bootstrap5",
    "type": "library",
    "description": "View for extension user with bootstrap5 css framework",
    "keywords": [
        "yii3",
        "extension",
        "user",
        "boopstrap5"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yii-extension/alert-message-bootstrap5": "@dev",
        "yii-extension/user-settings": "^1.0@dev",
        "yiisoft/form": "^1.0@dev",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/translator": "^3.0@dev",
        "yiisoft/user": "^3.0@dev",
        "yiisoft/yii-view": "^3.0@dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3.0",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\Tests\\": "tests"
        }
    },
    "provide": {
        "yii-extension/user-view-implementation": "1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/terabytesoftw/yii-bootstrap5"
        }
    ]
}
